<script>
  function brInitHandler() {
    var relatedSearches = document.querySelectorAll(".bloomreach-section .br-related-query");
    if (relatedSearches.length > 0) {
      document.querySelector(".bloomreach-section").style.display = "block";
    }
  }
  window.addEventListener("load", brInitHandler);
</script>

