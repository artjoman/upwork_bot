  .bloomreach-section #br-related-searches-widget,
  .bloomreach-section .br-related-query {
    display: inline;
  }
  .bloomreach-section .br-headline,
  .bloomreach-section a,
  .bloomreach-section .br-related-query:not(:last-of-type):after {
    color: #7d7d7d;
    font-family: 'Gotham SSm';
    font-size: 12px;
    font-style: normal;
    font-weight: 400;
    line-height: 20px;
  }
  .bloomreach-section .br-headline {
    font-weight: bold;
  }
  .bloomreach-section a:hover {
    text-decoration: underline;
  }
  .bloomreach-section .br-related-query:not(:last-of-type):after {
    color: #b2b2b2;
    content: ""
  }
</style>

