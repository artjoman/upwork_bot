                                                                                                                                                        <li data-ng-class="{active: categoryLinksController.isSubcategoryActive(&quot;presentations&quot;)}">
                            <a data-o-js-prevent-default="" data-ng-click="categoryLinksController.applySubcategoryFilter(&quot;presentations&quot;)" href="/o/jobs/browse/c/design-creative/sc/presentations/" class="visitor-category-link" data-slug="presentations">Presentations
                            </a>
                            <span data-ng-cloak="" class="text-muted">({{ categoryLinksController.getSubcategoryFacetCount("presentations")|number:0}})</span>
                        </li>
                                                                                                                        </ul>
        </div>
    </div>

            </div>
            <div data-category-links="" data-ng-if="moreResultsLoaded" class="m-0-top" data-ng-cloak=""></div>
        </div>
    </div>

