  .bloomreach-section .br-headline {
    font-weight: bold;
  }
  .bloomreach-section a:hover {
    text-decoration: underline;
  }
  .bloomreach-section .br-related-query:not(:last-of-type):after {
    color: #b2b2b2;
    content: "•"
  }
</style>

