ull,&quot;feedbackText&quot;:&quot;No feedback yet&quot;,&quot;companyOrgUid&quot;:&quot;970444004518797312&quot;},&quot;freelancersToHire&quot;:0,&quot;relevanceEncoded&quot;:&quot;{\&quot;$bucket\&quot;:\&quot;100.0\&quot;,\&quot;$hours_inactive\&quot;:\&quot;4.0\&quot;,\&quot;$lucene_terms_score\&quot;:\&quot;1.0\&quot;,\&quot;client_total_jobs_with_hires\&quot;:\&quot;0\&quot;,\&quot;job_value_group\&quot;:\&quot;LV\&quot;,\&quot;position\&quot;:\&quot;12\&quot;,\&quot;score\&quot;:\&quot;1.0\&quot;,\&quot;total_applicants\&quot;:\&quot;0\&quot;,\&quot;total_recommended\&quot;:\&quot;0\&quot;}&quot;,&quot;enterpriseJob&quot;:false,&quot;tierText&quot;:&quot;Entry level ($)&quot;,&quot;tier&quot;:&quot;$&quot;,&quot;tierLabel&quot;:&quot;Entry level&quot;,&quot;isSaved&quot;:null,&quot;feedback&quot;:null,&quot;proposalsTier&quot;:&quot;Less than 5&quot;,&quot;isApplied&quot;:false,&quot;sticky&quot;:false,&quot;stickyLabel&quot;:&quot;&quot;,&quot;jobTs&quot;:&quot;1551497284644&quot;,&quot;prefFreelancerLocationMandatory&quot;:true,&quot;prefFreelancerLocation&quot;:[&quot;United States&quot;],&quot;premium&quot;:false,&quot;plusBadge&quot;:null,&quot;publishedOn&quot;:&quot;2019-03-02T03:28:04+00:00&quot;,&quot;renewedOn&quot;:null,&quot;sandsService&quot;:null,&quot;sandsSpec&quot;:null,&quot;sandsAttrs&quot;:null,&quot;occupation&quot;:{&quot;uid&quot;:&quot;1044578476142100528&quot;,&quot;prefLabel&quot;:&quot;Scriptwriting&quot;},&quot;attrs&quot;:[{&quot;name&quot;:&quot;screenwriting&quot;,&quot;skillType&quot;:1,&quot;uid&quot;:&quot;475721704172060679&quot;,&quot;highlighted&quot;:false,&quot;prettyName&quot;:&quot;Screenwriting&quot;}],&quot;isLocal&quot;:false,&quot;workType&quot;:null,&quot;locations&quot;:[]})" id="job_~0188b4360e088f0c84" itemscope="">
