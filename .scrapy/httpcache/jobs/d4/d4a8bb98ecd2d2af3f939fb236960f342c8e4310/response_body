    var angularCache = [];
    var assetsVersion = "1f844b7";
    var assetsBaseUri = "\/static\/jsui\/JobSearchUI\/assets";
    var instanceEnv = "prod";

    var buildBundlePath = function(path) {
        var bundleRegex = /^@([^\/]*)Bundle\/(.*)$/;
        var matches = bundleRegex.exec(path);

        if (matches != null) {
            return '/bundles/' + matches[1].toLowerCase() + '/' + matches[2];
        }
        return path;
    };

    this.getBasePath = function() {
        return basePath;
    };

    this.buildInternalAssetPath = function(path) {
        // CDN assets
        if (assetsBaseUri) {
            return assetsBaseUri + '/' + assetsVersion + buildBundlePath(path);
        }

        // Local assets
        var url = basePath + buildBundlePath(path);
        if (assetsVersion) {
            if (url.indexOf('?') < 0) {
                return url + '?' + assetsVersion;
            } else {
                return url + '&' + assetsVersion;
            }
        }
        return url;
    };

    var visitorId = null;
        visitorId = "188.162.38.56.1551510261777165";
    
    this.getVisitorId = function() {
        return visitorId;
    };

    var visitorApiKey = null;
        visitorApiKey = "4e3ef7c585318b8422cb76907deb5cb1";
        this.getVisitorApiKey = function() {
        return visitorApiKey;
    };

    var User = null;
    
    this.getUser = function() {
        return User;
    };

        var Organization = null;
    
    this.getOrganization = function() {
        return Organization;
    };
    
    this.getVar = function(key) {
        return phpVars[key];
    };

    this.getAngularCache = function() {
        return angularCache;
    };

    this.getInstanceEnv = function() {
        return instanceEnv;
    };
};
